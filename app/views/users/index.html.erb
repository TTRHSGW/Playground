<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Username</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= link_to '見る', user %></td>
        <td><%= link_to '編集', edit_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'ユーザー新規作成', new_user_path%>
