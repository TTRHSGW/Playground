<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>



<%= form_with model: current_user.tweets.build do |f| %>
  <%= f.text_area :content %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.submit %>
<% end %>


<% if current_user.feed.any? %>
  <ul>
    <%  current_user.feed.each do |feed| %>
      <li><%= feed.content %></li>
    <% end %>
  </ul>
<% end %>
